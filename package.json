{
  "name": "@atwright147/react-layout",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "module": "./dist/react-layout.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-layout.js",
      "require": "./dist/react-layout.umd.cjs"
    }
  },
  "main": "./dist/react-layout.umd.cjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:lib": "tsc -p tsconfig.lib.json && vite build",
    "lint": "biome lint .",
    "build:demo": "tsc && vite build --mode demo",
    "preview": "vite preview --mode demo",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@storybook/builder-vite": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "globals": "^15.14.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Layout components for React applications",
  "homepage": "https://github.com/atwright147/react-lib-layout#readme",
  "bugs": {
    "url": "https://github.com/atwright147/react-lib-layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atwright147/react-lib-layout.git"
  },
  "license": "MIT",
  "author": "Andy Wright <atwright147@gmail.com>"
}
